#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/uglify-js/2.2.5/node_modules/uglify-js/bin/uglifyjs/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/uglify-js/2.2.5/node_modules/uglify-js/bin/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/uglify-js/2.2.5/node_modules/uglify-js/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/uglify-js/2.2.5/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/uglify-js/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/rev/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../uglify-js/2.2.5/node_modules/uglify-js/bin/uglifyjs" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/uglify-js/2.2.5/node_modules/uglify-js/bin/uglifyjs/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/uglify-js/2.2.5/node_modules/uglify-js/bin/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/uglify-js/2.2.5/node_modules/uglify-js/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/uglify-js/2.2.5/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/uglify-js/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/rev/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" node  "$basedir/../../../../../../uglify-js/2.2.5/node_modules/uglify-js/bin/uglifyjs" "$@"
  ret=$?
fi
exit $ret
