#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/acorn/1.2.2/node_modules/acorn/bin/acorn/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/acorn/1.2.2/node_modules/acorn/bin/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/acorn/1.2.2/node_modules/acorn/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/acorn/1.2.2/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/acorn/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/rev/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../acorn/1.2.2/node_modules/acorn/bin/acorn" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/acorn/1.2.2/node_modules/acorn/bin/acorn/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/acorn/1.2.2/node_modules/acorn/bin/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/acorn/1.2.2/node_modules/acorn/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/acorn/1.2.2/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/acorn/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/rev/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" node  "$basedir/../../../../../../acorn/1.2.2/node_modules/acorn/bin/acorn" "$@"
  ret=$?
fi
exit $ret
