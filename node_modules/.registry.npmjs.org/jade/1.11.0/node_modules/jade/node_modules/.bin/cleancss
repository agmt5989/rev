#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules/clean-css/bin/cleancss/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules/clean-css/bin/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules/clean-css/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/clean-css/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/rev/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../clean-css/3.4.28/node_modules/clean-css/bin/cleancss" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules/clean-css/bin/cleancss/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules/clean-css/bin/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules/clean-css/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/clean-css/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/rev/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" node  "$basedir/../../../../../../clean-css/3.4.28/node_modules/clean-css/bin/cleancss" "$@"
  ret=$?
fi
exit $ret
