#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/jade/1.11.0/node_modules/jade/bin/jade.js/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/jade/1.11.0/node_modules/jade/bin/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/jade/1.11.0/node_modules/jade/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/jade/1.11.0/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/jade/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/rev/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules:/mnt/c/Users/USER/node/rev/node_modules/jade/bin/jade.js/node_modules:/mnt/c/Users/USER/node/rev/node_modules/jade/bin/node_modules:/mnt/c/Users/USER/node/rev/node_modules/jade/node_modules" "$basedir/node"  "$basedir/../jade/bin/jade.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/jade/1.11.0/node_modules/jade/bin/jade.js/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/jade/1.11.0/node_modules/jade/bin/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/jade/1.11.0/node_modules/jade/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/jade/1.11.0/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/jade/node_modules:/mnt/c/Users/USER/node/rev/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/rev/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules:/mnt/c/Users/USER/node/rev/node_modules/jade/bin/jade.js/node_modules:/mnt/c/Users/USER/node/rev/node_modules/jade/bin/node_modules:/mnt/c/Users/USER/node/rev/node_modules/jade/node_modules" node  "$basedir/../jade/bin/jade.js" "$@"
  ret=$?
fi
exit $ret
